    <text>
        <body>
            
            <pb facs="#PN84-0"/>
            
            <head type="rúbrica"> Esta <term cRef="162">pregunta</term>
                <term cRef="174">fizo</term> e <term cRef="175">ordenó</term> contra el dicho
                    <persName ref="#AAV">Alfonso Álvarez</persName> un <persName ref="#BSA">Bachiller en Artes de <placeName ref="#SA1">Salamanca</placeName>
            </persName>
            </head>

            <pb facs="#PN84-1"/>

            <sp who="#AAV">
                <speaker>Bachiller en Artes de Salamanca</speaker>
                <lg type="poema"
                subtype="pregunta"
                unisonancia="singular"
                isometrismo="isométrico"
                isoestrofismo="heteroestrófico"
                asonancia="consonante"
                met="8x10"
                rhyme="ABBAACCA">
                    <note> Se trata de una pieza de arte mayor</note>
                    <lg n="1"
                   type="octava"
                   met="8x10"
                   rhyme="ABBAACCA">

                        <l xml:id="L1">Non <rs ref="#BSA">tengo</rs> que siente la natural<rhyme label="eza">eza</rhyme>
                  </l>
                        <l xml:id="L2">nin puede sentir en sí la raz<rhyme label="on">ón</rhyme>
                  </l>
                        <l xml:id="L3">de omne prudente de grant discreci<rhyme label="on">ón</rhyme>
                  </l>
                        <l xml:id="L4">que menor de otro le sobre en alt<rhyme label="eza">eza</rhyme>
                  </l>
                        <l xml:id="L5">pero <rs ref="#BSA">yo</rs> bien creo de cierta firm<rhyme label="eza">eza</rhyme>
                  </l>
                        <l xml:id="L6">que menor de ángel le puja en est<rhyme label="ado">ado</rhyme>,</l>
                        <l xml:id="L7">
                            <rs ref="#BSA">deseo</rs> saber d’ algunt grant <term cRef="184">letr<rhyme label="ado">ado</rhyme>
                     </term>
                  </l>
                        <l xml:id="L8">quien es que meresce aver tal franqu<rhyme label="eza">eza</rhyme>.</l>
                    </lg>

                    <pb facs="#PN84-2"/>

                    <lg n="2"
                   type="octava"
                   met="8x10"
                   rhyme="ABBAACCA">
                        <l xml:id="L9">De otra quistión asaz bien esc<rhyme label="ura">ura</rhyme>
                  </l>
                        <l xml:id="L10">cobdiçio tanbién ser certific<rhyme label="ado">ado</rhyme>
                  </l>
                        <l xml:id="L11">que como podria estar engendr<rhyme label="ado">ado</rhyme>
                  </l>
                        <l xml:id="L12">un padre sin dubda de su fija p<rhyme label="ura">ura</rhyme>
                  </l>
                        <l xml:id="L13">que a mi paresce ser cosa muy d<rhyme label="ura">ura</rhyme>
                  </l>
                        <l xml:id="L14">ser antes la fija que nasca su p<rhyme label="adre">adre</rhyme>
                  </l>
                        <l xml:id="L15">o quando la pudo fazer en su m<rhyme label="adre">adre</rhyme>
                  </l>
                        <l xml:id="L16">pues que el non era in rerum nat<rhyme label="ura">ura</rhyme>.</l>
                    </lg>

                    <lg n="3"
                   type="octava"
                   met="8x10"
                   rhyme="ABBAACCA">
                        <l xml:id="L17">La quistión tercera es maravill<rhyme label="osa">osa</rhyme>
                  </l>
                        <l xml:id="L18">por ser muy dubdada en la asoluci<rhyme label="on">ón</rhyme>
                  </l>
                        <l xml:id="L19">ca deve tener sotil coraç<rhyme label="on">ón</rhyme>
                  </l>
                        <l xml:id="L20">quien adevinare tan escura c<rhyme label="osa">osa</rhyme>
                  </l>
                        <l xml:id="L21">un padre casado ya con su esp<rhyme label="osa">osa</rhyme>
                  </l>
                        <l xml:id="L22">en tanto que vivo non pudo engendr<rhyme label="ar">ar</rhyme>
                  </l>
                        <l xml:id="L23">después que fue muerto del nasció su p<rhyme label="ar">ar</rhyme>
                  </l>
                        <l xml:id="L24">un fijo gracioso mas fino que r<rhyme label="osa">osa</rhyme>.</l>
                    </lg>

                    <lg n="4"
                   type="octava"
                   met="8x10"
                   rhyme="ABBAACCA">
                        <l xml:id="L25">La quarta pregunta es tan intric<rhyme label="ada">ada</rhyme>
                  </l>
                        <l xml:id="L26">que la discretiva se turba en prov<rhyme label="iso">iso</rhyme>
                  </l>
                        <l xml:id="L27">aunque sea omne discreto e env<rhyme label="iso">isso</rhyme>
                  </l>
                        <l xml:id="L28">fasta la memoria estar soseg<rhyme label="ada">ada</rhyme>
                  </l>
                        <l xml:id="L29">seyendo la madre encinta preñ<rhyme label="ada">ada</rhyme>
                  </l>
                        <l xml:id="L30">el fijo al padre mató cruelm<rhyme label="ente">ente</rhyme>
                  </l>
                        <l xml:id="L31">cosa verdadera fue esto en Ori<rhyme label="ente">ente</rhyme>
                  </l>
                        <l xml:id="L32">en una cibdat notable esmer<rhyme label="ada">ada</rhyme>.</l>
                    </lg>

                    <lg n="5" type="finida" met="4x10" rhyme="ABBA">
                        <l xml:id="L33">
                     <rs ref="#AAV">Quien</rs> diere <term cRef="163">respuesta</term>bien <term cRef="168">metrific<rhyme label="ada">ada</rhyme>
                     </term>
                  </l>
                        <l xml:id="L34">será ensalçado entre <term cRef="183">sabid<rhyme label="ores">ores</rhyme>
                     </term>,</l>
                        <l xml:id="L35">
                            <term cRef="182">poetas famossos</term> darán d’el lo<rhyme label="ores">ores</rhyme>
                  </l>
                        <l xml:id="L36">si naturalmente es abtoriz<rhyme label="ada">ada</rhyme>.</l>
                    </lg>
                </lg>
            </sp>
        </body>
